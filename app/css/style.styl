@import 'rupture'
@import "variable/_function.styl"
@import "variable/_ie.styl"
@import "variable/_fonts.styl"
@import "variable/_helper.styl"
@import "variable/_default.styl"
@import "variable/_elements.styl"
@import "variable/_constant.styl"
//@import "variable/_grid.styl"

rupture.scale = 0 480px 768px 992px 1200px
rupture.scale-names = 'xs' 'sm' 'md' 'lg'

// Grid breakpoints
$breakpoints = {
	'xs' :  0,
	'sm' : 480px,
	'md' : 768px,
	'lg' : 992px,
	'xl' : 1200px
}

// Number of columns
$columns = 12

init($breakpoints, $columns)

body
	@extend .type--main
	font-family $v-font
	//color $main-text-color
	line-height 1.5
	letter-spacing 0.01em
	
.main-cont
	center(1200px,10px)

//==== content-tool ====
.content-tool
	flex(between)
	fw()
	padding: 25px 0
	+below(640px)
		padding: 20px 0

.bread-wrap
	border-bottom 1px solid rgba(0, 0, 0, 0.1)

.breadcrumbs
	+below(1024px)
		width: 100%
		margin-bottom: 20px
		flex(mid)
	+below(768px)
		display: none
	f()

.content
	position: relative

.breadcrumbs__el
	margin-right: 20px
	position: relative
	&:last-child
		margin-right: 0
		&:after
			content ''
			display: none
	&:after
		content '>'
		position: absolute
		right: -15px
.back
	+below(1024px)
		flex(mid)
		width: 100%
	f()
	justify-content: center
	.icon
		size 17px
		fill #2f2f2f
		margin-right: 10px
.back__text
	line-height: 1.2
// ==== content-tool-end ====

.page-title
	+below(1024px)
		margin-top: 0
		margin-bottom: 25px
	+below(768px)
		font-size: 26px
	+below(640px)
		margin-bottom: 15px
		line-height: 1
		font-size: 22px
	@extend .type--xl
	font-weight 500
	color black
	text-align: center
	margin-top: 20px
	margin-bottom: 70px

.list-cont
	fw()
	f()
	justify-content: space-between
	align-items: flex-start

.list-tool
	+below(1024px)
		width: 100%
	+below(1024px)
		display: none
	width: 240px

.filter
	margin-bottom: 30px

.filter-clear
	+below(1024px)
		margin-bottom: 30px
	f()
	justify-content: flex-end
	align-items: center
	color $red
	margin-bottom: 70px
	cp()
	.icon
		margin-left: 5px
		transform rotate(45deg)
		size 10px
		fill $red

.filter__el
	border 1px solid #ebebeb
	margin-top: -1px
	//background: #f9f9f9

.filter__head
	flex(between)
	cp()
	padding: 20px

.filter__title
	color #070707

.filter__open
	flex(mid)
	cp()
	tr(all)
	.icon
		size 18px
		fill black
.filter__open--active
	transform rotate(315deg)

.filter__content
	margin-top: 20px
	display: none
	padding: 0 20px 20px 20px
	.check-el
		margin-bottom: 15px
		&:last-child()
			margin-bottom: 0px
.filter__size
	+below(1024px)
		flex(mid)

	display: grid
	grid-template-columns repeat(auto-fill, 40px)
	grid-gap 10px

.filter__size-el
	flex(mid)
	tr(all)
	width: 40px
	height: 30px
	border 1px solid #e1e1e1
	background-color white
	&:hover
		color white
		cursor: pointer
		border 0px
		background: black

.filter__size-el--active
	color white
	cursor: pointer
	border 0px
	background: black

.filter__color
	+below(768px)
		f()
		fw()
	justify-content center
	display grid
	grid-template-columns repeat(auto-fit, 42px)
	grid-gap 10px
.filter__color-el--active
	.filter__color-ico
			opacity 1

.filter__color-ico
	tr(all)
	.icon
		size 17px
		fill white
	opacity 0

.filter__color-el--white
	border 1px solid #e1e1e1
	.icon
		fill black
.filter__color-el
	+below(768px)
		margin: 5px
	flex(mid)
	tr(all)
	size 42px
	round()
	cp()
	&:hover
		.filter__color-ico
			opacity 1
//.filter__season
//.filter__collect
.list-content
	+below(1024px)
		width: 100%
	width:calc(100% - 280px)

.list-category-title
	@extend .type--md
	font-weight 600
	margin-bottom: 40px
	color black

.list-category
	+below(1024px)
		flex(between)
		fw()
		margin-bottom: 30px
.list-category__el
	+below(1024px)
		width: 48%
	+below(640px)
		width: 100%
	margin-bottom: 20px
	color black
	line-height: 1.6
	display: block
	span
		display: inline-block
		color #828282

.nice-select
	br(0)
	display: block
	color #000000
	font-size 14px
	outline-color white!important
	box-shadow 0 1px 2px rgba(0, 0, 0, 0.21)
	border 1px solid #e9e9e9
	.list
		width: calc(100% )
		left 0px
		br(0)
		border 1px solid #dfdfdf
		border-top 0
		margin-top: 0
		li
			f()
			align-items: center
			max-width: 100%
			text-overflow: ellipsis
			padding-right: 10px
			overflow hidden

.element-select
	padding: 10px 0

.nice-select:active, .nice-select.open, .nice-select:focus
	border 1px solid #dfdfdf

.nice-select.open
	border 0!important
	box-shadow 0 5px 24px rgba(0, 0, 0, 0.04)
	.list
		border 0!important
		box-shadow 0 5px 24px rgba(0, 0, 0, 0.04)

.select--order
	width 240px

