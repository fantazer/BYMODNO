.card-attr__cont--half
	+below(640px)
		grid-template-columns 1fr

	display: grid
	grid-template-columns 45% 45%

.card-review__el
	padding-bottom: 40px
	margin-bottom: 40px
	border-bottom 1px solid #dbdbdb
	&:last-child
		border 0
		padding-bottom:0
		margin-bottom: 0

.card-review__head
	+below(640px)
		fw()
	flex(between)
	margin-bottom: 10px
.card-review__info
	+below(640px)
		width: 100%
		margin-bottom: 10px
	f()
	align-items: center
.card-review__name
	font-size: 18px
	font-weight:600
	margin-right: 30px
.card-review__stars
	f()
.card-review__stars-el
	margin-right: 5px
	.icon
		size 16px
.card-review__stars---true
	.icon
		fill $red
.card-review__stars---false
	.icon
		fill #c7c7c7
.card-review__date
	+below(640px)
		width: 100%
//.card-review__text
.card-review__img
	f()
	fw()
.card-review__img-el
	+below(640px)
		size 40px
	bg-full()
	size 80px
	margin-right: 10px

.card-attr-review
	f()
	fw()
	justify-content: space-between
	align-items: flex-start

.card-attr-review-cont
	+below(768px)
		width: 100%
		margin-bottom: 20px
	width:calc(100% - 300px)
.card-attr-review-add
	+below(768px)
		width: 100%
	width: 210px
//.card-review-add
.card-review-add__title
	font-family: "Gotham"
	font-size: 18px
	font-weight: 500
	margin-bottom: 10px
.card-review-add__text
	color: #5a5a5a;
	font-family: Arial;
	margin-bottom: 45px
.card-review-add__get
	@extend .btn--md
	@extend .btn--mark
	width: 100%
	padding: 20px

.card-param
	+below(640px)
		padding: 10px
		margin-top: 10px
	padding: 20px 0
	margin-top: 20px
	border-top 1px solid rgba(112,112,112,0.15)
	width: 100%
.card-param__text
	+below(640px)
		text-align: center
	text-align: right
	margin-bottom: 10px
	&:last-child
		margin-bottom: 0
.card-img__main-wrap
	&:hover
		.card-img__main-control
				visibility: visible
				opacity 1
	position: relative
.card-img__main-control
	+below(1024px)
		visibility visible
		opacity 1
	+below(640px)
		display: none
	tr(all)
	width: 100%
	position: absolute
	z-index 10
	top calc(50% - 10px)
	visibility: hidden
	opacity 0
.card-img__main-control__el
	flex(mid)
	round()
	size 36px
	background: black
	cp()
	.icon
		size 20px
		fill white
.card-img__main-control__el--prev
	left -15px
	position: absolute
.card-img__main-control__el--next
	right: -15px
	position: absolute
.card-img__main
	height: 100%

.trust
	+below(640px)
		display: none
	flex(between)
	padding: 60px 0 40px 0
.trust__text
	color #636363
	font-family Gotham
	font-size 24px
	font-weight 500
	text-align: right
	width: 15%
.trust__logo
	flex(between)
	width: 80%
.trust__logo__el
	width: 100%
	flex-grow 1
	flex(mid)
	margin: 0 20px

@import "../module/**/*"
